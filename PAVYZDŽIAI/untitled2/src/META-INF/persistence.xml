<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="NewPersistenceUnit">
        <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
        <mapping-file>gyvunas/GyvunaEntity.xml</mapping-file>
        <mapping-file>gyvunas/GyvunasEntity.xml</mapping-file>
        <mapping-file>gyvunas/KambarysEntity.xml</mapping-file>
        <mapping-file>gyvunas/SuoEntity.xml</mapping-file>
        <class>gyvunas.GyvunaEntity</class>
        <class>gyvunas.GyvunasEntity</class>
        <class>gyvunas.KambarysEntity</class>
        <class>gyvunas.SuoEntity</class>
        <properties>
            <property name="openjpa.ConnectionURL" value="jdbc:mysql://localhost:3306/test"/>
            <property name="openjpa.ConnectionDriverName" value="com.mysql.jdbc.Driver"/>
            <property name="openjpa.ConnectionUserName" value=""/>
            <property name="openjpa.ConnectionPassword" value=""/>
            <property name="openjpa.Log" value="DefaultLevel=TRACE, Tool=INFO"/>
        </properties>
    </persistence-unit>
</persistence>
